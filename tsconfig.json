{
	"compilerOptions": {
		"strict": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"target": "ESNext",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"types": ["vite/client", "react", "react-dom"],
		"jsx": "react-jsx",
		"skipLibCheck": true,
		"esModuleInterop": true
	},
	"include": ["node_modules/vite/client.d.ts", "**/*", "src", "api"],
	"exclude": ["dist"]
}
